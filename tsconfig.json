{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "esnext",
    "sourceMap": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strictPropertyInitialization": false,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strict": true,
    "noEmit": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": ["node", "jest", "k6"]
  },
  "exclude": [
    "node_modules",
  ],
  "include": ["src"]
}
